@InProceedings{Woods2021,
	author = {Woods, Eoin and Bashroush, Rabih},
	booktitle = {Software {Architecture} - 15th European Conference, {ECSA} 2021},
	title = {How {Software} {Architects} {Focus} {Their} {Attention}},
